@model JAPhotography.Models.DirImagesModel

@if (!(Model.Thumbnails == null && Model.Images == null))
{
    <div id="thumbnail_gallery" class="justified-gallery">
        @for (int i = 0; i < Model.Thumbnails.Count(); i++)
        {
            <a href="@Url.Content(Model.Images[i])" class="darken">
                <img src="@Url.Content(Model.Thumbnails[i])" alt="" />
            </a>
        }
    </div>
}